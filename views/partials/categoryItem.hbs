<li>
    <a href={{href}}>
        <h2>Content</h2>
        <img src="/images/nextArrow.svg" alt="next arrow">
        <section class='progress'>
            <progress>
                value={{round
                (multiply
                (divide this.completedChecklists this.checkLists.length)
                100
                )
                }}
                min='0'
                max='100'
            </progress>
            {{! <3 handlebars }} </section>
                <p>{{round
                    (multiply
                    (divide this.completedChecklists this.checkLists.length)
                    100
                    )
                    }}% accessible</p>
        <p>1/3</p>
    </a>
</li>